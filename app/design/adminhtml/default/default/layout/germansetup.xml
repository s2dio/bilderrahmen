<layout version="0.1.0">
    <default>
        <reference name="notifications">
            <block type="germansetup/adminhtml_notifications" name="germansetup_notifications" template="germansetup/notifications.phtml" before="-"></block>
        </reference>
    </default>
	<adminhtml_germansetup_index>
        <reference name="content">
            <block type="germansetup/adminhtml_germansetup" name="germansetup.index" template="germansetup/form.phtml"/>
        </reference>
    </adminhtml_germansetup_index>
	<adminhtml_germansetup_extensions>
        <reference name="content">
            <block type="adminhtml/template" name="germansetup.extensions" template="germansetup/extensions.phtml"/>
        </reference>
    </adminhtml_germansetup_extensions>
</layout>
